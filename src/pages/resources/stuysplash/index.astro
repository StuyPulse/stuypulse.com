---
import { getCollection } from 'astro:content';

const StuySplashEntries = await getCollection('stuysplash');
const latestEntry = StuySplashEntries[StuySplashEntries.length - 1];
const latestYear = latestEntry.slug;

const url = `0; url=/resources/stuysplash/${latestYear}`;
---

<meta http-equiv="refresh" content={url} />
