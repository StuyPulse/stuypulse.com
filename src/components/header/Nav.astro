---
import type {NavType} from '../../types';

export interface Props {
  nav: NavType;
}

const { nav } = Astro.props;
---
<li class="nav-item">
   {nav.dropdown ? 
   <a class="nav-link active text-black" aria-current="page" href= { nav.href }>{ nav.name }</a> :
   <a class="nav-link active text-white" aria-current="page" href= { nav.href }>{ nav.name }</a>}
</li>
